{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "semi": "error",
    "jsx-a11y/anchor-is-valid": [
      0
    ],
    "react/jsx-filename-extension": [
      0
    ],
    "react/prop-types": [
      0
    ],
    "react/react-in-jsx-scope": [
      0
    ]
  }
}
